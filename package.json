{
  "name": "workflow-test",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "predeploy": "claudia generate-serverless-express-proxy --express-module server",
    "deploy": "npm run predeploy && exec bin/claudia-deploy.sh",
    "update": "npm run predeploy && TMPDIR=/tmp claudia update --set-env-from-json"
  },
  "dependencies": {
    "aws-sdk": "^2.255.1",
    "aws-serverless-express": "^3.2.0",
    "body-parser": "^1.18.3",
    "claudia": "^5.0.0",
    "connect-dynamodb": "^2.0.0",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.9",
    "dotenv": "^6.0.0",
    "dynamodb": "^1.1.2",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "express-validator": "^5.2.0",
    "helmet": "^3.12.1",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.22.2",
    "morgan": "^1.9.0",
    "nodemon": "^1.17.5",
    "request-promise": "^4.2.2",
    "simple-oauth2": "^1.6.0"
  }
}
